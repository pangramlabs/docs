{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Pangram Labs",
  "colors": {
    "primary": "#FF6106",
    "light": "#FF6106",
    "dark": "#FECAB9"
  },
  "favicon": "/favicon.svg",
  "navigation": {
    "tabs": [
      {
        "tab": "Guides",
        "groups": [
          {
            "group": "Getting Started",
            "pages": [
              "index",
              "quickstart",
              "quickstart-rest"
            ]
          }
        ]
      },
      {
        "tab": "Python SDK",
        "groups": [
          {
            "group": "Reference",
            "pages": [
              "sdk/python"
            ]
          }
        ]
      },
      {
        "tab": "REST API",
        "groups": [
          {
            "group": "REST API",
            "pages": [
              "api-reference/introduction",
              "api-reference/ai-detection",
              "api-reference/plagiarism-detection"
            ]
          },
          {
            "group": "Deprecated",
            "pages": [
              "api-reference/deprecated-endpoints"
            ]
          }
        ]
      }
    ]
  },
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg"
  },
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "mailto:support@pangram.com"
      }
    ]
  },
  "api": {
    "mdx": {
      "auth": {
        "method": "key",
        "name": "x-api-key"
      }
    },
    "playground": {
      "display": "interactive"
    },
    "examples": {
      "languages": ["curl", "python"],
      "defaults": "required"
    }
  },
  "footer": {
    "socials": {}
  }
}
